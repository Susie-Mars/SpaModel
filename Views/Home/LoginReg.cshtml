@{
    ViewData["Title"] = "Login/Register";
}



@await Html.PartialAsync("_Navbar")

<div class="AllServicesDesc">
    <div class="d-flex flex-column">
        <div>
            <h3>Please Login or Register to Post</h3>
        </div>
        <div class="d-flex m-2 gap-5">
            <div>
                <h5>Login</h5>
                @await Html.PartialAsync("_Login")
            </div>
            <div>
                <h5>Register</h5>
                @await Html.PartialAsync("_Register")
            </div>


        </div>
    </div>
</div>
@await Html.PartialAsync("_Footer")
